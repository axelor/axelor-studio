<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="studio" package="com.axelor.studio.db"/>

  <entity name="AccessConfig" cacheable="true">
    <string name="name" readonly="true"/>
    <many-to-one name="app" ref="com.axelor.studio.db.App" title="App"/>
    <many-to-many name="roleSet" ref="com.axelor.auth.db.Role" title="Roles"/>
    <unique-constraint columns="app,name"/>
  </entity>

</domain-models>
