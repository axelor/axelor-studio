<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="studio" package="com.axelor.studio.db"/>

  <entity name="BamlModel" cacheable="true">

    <string name="name" title="Name" required="true"/>
    <string name="bamlXml" title="Input xml" large="true"/>
    <string name="resultScript" title="Result script" large="true"/>
    <many-to-one name="studioApp" ref="com.axelor.studio.db.StudioApp" title="App"/>

  </entity>

</domain-models>
