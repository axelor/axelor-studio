<?xml version="1.0" encoding="utf-8"?>
<xml-inputs xmlns="http://axelor.com/xml/ns/data-import"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/data-import https://axelor.com/xml/ns/data-import/data-import_7.2.xsd">

  <input file="wkf-dmn-model.xml" root="wkf-dmn-models">

    <bind node="wkf-dmn-model" type="com.axelor.studio.db.WkfDmnModel"
      search="self.name = :name" update="true">
      <bind node="name" to="name"/>
      <bind node="description" to="description"/>
      <bind node="diagramXml" to="diagramXml"/>
      <bind node="metaModels/metaModel" to="metaModelSet" search="self.name in :name"
        create="false">
        <bind node="@name" to="name"/>
      </bind>
      <bind node="jsonModels/jsonModel" to="jsonModelSet" search="self.name in :name"
        create="false">
        <bind node="@name" to="name"/>
      </bind>
      <bind node="studioApp" to="studioApp" search="self.code = :studioApp" create="false"/>
    </bind>

  </input>

</xml-inputs>