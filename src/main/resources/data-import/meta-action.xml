<?xml version="1.0" encoding="utf-8"?>
<xml-inputs xmlns="http://axelor.com/xml/ns/data-import"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/data-import https://axelor.com/xml/ns/data-import/data-import_7.3.xsd">

  <input file="meta-action.xml" root="meta-actions">

    <bind node="meta-action" type="com.axelor.meta.db.MetaAction"
      search="self.name = :name AND (self.xmlId is null OR self.xmlId = :xmlId)  AND (self.module is null OR self.module = :module)"
      update="true">
      <bind node="name" to="name"/>
      <bind node="type" to="type"/>
      <bind node="model" to="model"/>
      <bind node="module" to="module"/>
      <bind node="xml" to="xml"/>
      <bind node="xmlId" to="xmlId"/>
      <bind node="studioApp" to="studioApp" search="self.code = :studioApp" create="false"/>
    </bind>

  </input>

  <input file="meta-action.xml" root="meta-actions">
    <bind node="meta-action" type="com.axelor.meta.db.MetaAction"
      search="self.name = :name AND (self.xmlId is null OR self.xmlId = :xmlId) AND (self.module is null OR self.module = :module)"
      update="true" create="false">
      <bind node="name" to="name"/>
      <bind node="xmlId" to="xmlId"/>
      <bind node="module" to="module"/>
    </bind>

  </input>

</xml-inputs>