  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.1.xsd">

  <grid name="link-script-param-grid" title="LinkScript Parameters" edit-icon="false"
    orderBy="id" model="com.axelor.studio.db.LinkScriptParam" editable="true" x-selector="checkbox">
    <field name="name"/>
  </grid>

  <form name="link-script-param-form" title="LinkScript Parameter"
