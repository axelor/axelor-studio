<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views https://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid name="wkf-instance-variable-grid" title="Instance Variables"
    model="com.axelor.studio.db.WkfInstanceVariable">
    <field name="name"/>
    <field name="value"/>
  </grid>
  <form name="wkf-instance-variable-form" title="Instance Variable"
    model="com.axelor.studio.db.WkfInstanceVariable">
    <panel name="mainPanel">
      <field name="name"/>
      <field name="value"/>
    </panel>
  </form>
</object-views>
