<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views https://axelor.com/xml/ns/object-views/object-views_7.4.xsd">

  <selection name="bpm.model.status.select">
    <option value="1">New</option>
    <option value="2">On Going</option>
    <option value="3">Terminated</option>
  </selection>

  <selection name="chart.wkf.bpm.manager.type.select">
    <option value="lateTask">Late tasks</option>
    <option value="nonLateTask">Not late tasks</option>
  </selection>

  <selection name="chart.wkf.bpm.manager.unit.select">
    <option value="minutes">Minutes</option>
    <option value="hours">Hours</option>
    <option value="days">Days</option>
  </selection>

  <selection name="studio.app.type.select">
    <option value="standard">Standard</option>
    <option value="addons">Addons</option>
    <option value="enterprise">Enterprise</option>
    <option value="pro">Pro</option>
    <option value="custom">Custom</option>
    <option value="others">Others</option>
    <option value="customer">Customer</option>
  </selection>

  <selection name="wkf.instance.migration.status.select">
    <option value="1">Not migrated</option>
    <option value="2">Migrated successfully</option>
    <option value="3">Migration error</option>
  </selection>

  <selection name="wkf.instance.migration.related.to.select">
    <option value="com.axelor.studio.db.WkfModel">WkfModel</option>
  </selection>

  <selection name="studio.menu.view.param.type.select">
    <option value="forceEdit">forceEdit</option>
    <option value="forceTitle">forceTitle</option>
    <option value="showArchived">showArchived</option>
    <option value="details-view">details-view</option>
    <option value="search-filters">search-filters</option>
    <option value="default-search-filters">default-search-filters</option>
    <option value="limit">limit</option>
    <option value="popup">popup</option>
    <option value="popup-save">popup-save</option>
    <option value="popup.maximized">popup.maximized</option>
    <option value="show-toolbar">show-toolbar</option>
    <option value="show-confirm">show-confirm</option>
    <option value="reload-dotted">reload-dotted</option>
    <option value="download">download</option>
  </selection>

  <selection name="studio-menu-view-param-boolean-value">
    <option value="true">True</option>
    <option value="false">False</option>
  </selection>

  <selection name="studio-wkf-process-update-status-select">
    <option value="1">Draft</option>
    <option value="2">Applied</option>
    <option value="3">Error</option>
  </selection>

  <selection name="studio-wkf-instance-log-filter-select">
    <option value="1">Date</option>
    <option value="2">Duration</option>
    <option value="3">All</option>
  </selection>
  <selection name="studio.app.bpm.custom.variable.status">
    <option value="1">Valid</option>
    <option value="2">Invalid</option>
  </selection>

  <selection name="wkf.instance.status.select">
    <option value="1">In progress</option>
    <option value="2">Stopped</option>
  </selection>

</object-views>
