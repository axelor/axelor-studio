<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.1.xsd">

  <form name="custom-variable-form" title="Custom Variable"
    model="com.axelor.studio.db.CustomVariable"
    onSave="action-method-custom-variable-set-status,save">
    <panel name="customVariablePanel">
      <field name="name"/>
      <field name="title"/>
      <field name="expression" widget="CodeEditor" height="60"/>
    </panel>
  </form>
  <grid name="custom-variable-grid" title="Custom Variables"
    model="com.axelor.studio.db.CustomVariable" canNew="true">
    <field name="name"/>
    <field name="title"/>
    <field name="expression"/>
    <field name="status"/>
  </grid>



</object-views>
